{
    "manifest_version": 3,
    "name": "Rock Paper Scissor Game",
    "version": "1.0",
    "description": "Simple Rock Paper Scissor game to play in free time for getting extra bored :(",
    "permissions": [
      "storage"
    ],
    "action": {
      "default_popup": "index.html"
    },
    "web_accessible_resources": [
      {
        "resources": ["index.html"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  
